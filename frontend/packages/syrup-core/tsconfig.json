{
  "compilerOptions": {
    "target": "es6",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxImportSource": "@emotion/react",
    "types": ["vite/client", "@emotion/react/types/css-prop", "@emotion/react", "@emotion/styled"]
  },
  "include": [
    "src",
    "../index.ts",
    "Animation",
    "components/Built/ContactButtons",
    "components/Built/DisplayTable",
    "components/Built/ImageHeader",
    "components/Built/Pagination",
    "Built/PalettePreview",
    "components/Built/ScrollToTopFAB",
    "components/Built/index.ts",
    "components/Containers",
    "components/Buttons/BrandButton",
    "components/Buttons/Button",
    "Buttons/FAB",
    "components/Buttons/IconButton",
    "components/Buttons/SpeedDial",
    "components/Buttons/Tabs",
    "components/Buttons/ToggleButton",
    "components/Buttons/index.ts",
    "components/Layout",
    "components/Elements/Accordion",
    "components/Elements/Avatar",
    "components/Elements/Breadcrumbs",
    "components/Elements/Divider",
    "components/Elements/Drawer",
    "components/Elements/Gallery",
    "components/Elements/Icon",
    "components/Elements/IconTextItem",
    "components/Elements/Link",
    "components/Elements/List",
    "components/Elements/Loading",
    "components/Elements/Media",
    "components/Elements/Menu",
    "components/Elements/Modal",
    "components/Elements/Navbar",
    "components/Elements/Popover",
    "components/Elements/Sidebar",
    "components/Elements/Stepper",
    "components/Elements/Table",
    "components/Elements/Tag",
    "components/Elements/Text",
    "components/Elements/Tooltip",
    "components/Elements/Tree",
    "components/Elements/index.ts",
    "components/Form/Checkbox",
    "components/Form/ImageInput",
    "components/Form/Input",
    "components/Form/Option",
    "components/Form/Radio",
    "components/Form/RichTextInput",
    "components/Form/Select",
    "components/Form/Switch",
    "components/Form/index.ts",
    "hooks",
    "types",
    "index.ts",
    "theme/base",
    "theme/palettes",
    "theme/common",
    "theme/types",
    "theme/utils",
    "theme/index.ts"
  ],
  "exclude": ["node_modules"],
  "extends": "./tsconfig.paths.json"
}
